<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="id.qibee.newsassistant.ui.home.HomeFragment"
        android:label="@string/title_headline"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_trending"
        android:name="id.qibee.newsassistant.ui.dashboard.DashboardFragment"
        android:label="@string/title_trending"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_subscriptions"
        android:name="id.qibee.newsassistant.ui.notifications.NotificationsFragment"
        android:label="@string/title_subscriptions"
        tools:layout="@layout/fragment_notifications" />
</navigation>